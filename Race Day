let raceNumber = Math.floor(Math.random() * 1000);
let early = true;
let age = 18;
if (early && age> 18 ) {
  raceNumber += 1000;
}
if (early && age >18) {
  console.log(`Dear ${raceNumber}, you will race at 9:30am.`);
}
  else if (age >18 && !early) {
    console.log(`Dear ${raceNumber}, you will race at 11:00am.`);
  }
else if (age < 18) {
  console.log(`Dear ${raceNumber}, you will race at 12:30pm.`);
}
else {
  console.log("Please approach staff at service desk.");
}
