//Setting a variable for my age.
let myAge = 20;
//calculation for the early 2 years of a dog
let earlyYears = 2;
earlyYears = earlyYears *10.5;
//We will work on the years after the 1st 2 years.
let laterYears = myAge - 2;
//Multiplying the lateryears by 4
laterYears *=4;
console.log(earlyYears);
console.log(laterYears);
//Adding earlyYears and LaterYears to find my age in dog years.
let myAgeInDogYears= earlyYears + laterYears;


//changing the form of my name to all lower case
let myName= "Kerry".tolowerCase;

console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} in dog years.`);
